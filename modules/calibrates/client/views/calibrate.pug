extends ../../../core/client/views/layout

block page
  - var menu = 'calibrates'
block content
  div
    h3='Calibrate Listing'
    - if (calibrates.length) {
    table.table
      tr
        th id
        th customer_id
        th Assembly_number
        th revision
        th unique_key
      each calibrate in calibrates
        tr
          td= calibrate.id
          td= calibrate.customer_id
          td= calibrate.number
          td= calibrate.revision
          - if (calibrate.unique_key) {
          td= calibrate.unique_key
          - }
    - }